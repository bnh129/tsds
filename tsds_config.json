{
  "storage": {
    "base_path": "./tsds_storage",
    "wal_path": "wal",
    "cold_path": "cold",
    "logs_path": "logs"
  },
  "hot_tier": {
    "max_records": 1000000,
    "eviction_threshold_pct": 0.9,
    "wal_enabled": true
  },
  "warm_tier": {
    "max_memory_mb": 2048,
    "eviction_threshold_pct": 0.9,
    "wal_enabled": true
  },
  "cold_tier": {
    "compression": "snappy",
    "index_batch_interval": 1.0,
    "staging_enabled": true,
    "max_thread_workers": 8
  },
  "query": {
    "output_batch_size": 4096,
    "max_concurrent_files": 10
  },
  "indexing": {
    "auto_index": false,
    "index_columns": ["timestamp", "symbol"],
    "batch_size": 1000
  },
  "schema": {
    "time_column": "timestamp"
  },
  "logging": {
    "level": "DEBUG",
    "max_file_size_mb": 100,
    "max_files": 10,
    "format": "%(asctime)s | %(name)s | %(levelname)s | %(funcName)s:%(lineno)d | %(message)s"
  },
  "performance": {
    "chunk_size": 10000,
    "parallel_ingestion": true,
    "memory_limit_mb": 8192,
    "gpu_memory_limit_mb": 2048
  },
  "debug": {
    "enabled": false,
    "print_tier_stats": false,
    "print_query_plans": false
  }
}
